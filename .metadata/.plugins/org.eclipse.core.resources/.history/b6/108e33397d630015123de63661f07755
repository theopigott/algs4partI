
@SuppressWarnings("unchecked")
public class StackArray<Item> {
	private Item[] a;
	int N = 0;
	
	public StackArray() {
		a = (Item[]) new Object[1];
	}
	
	public void push(Item item) {
		if (N == a.length) resize(N * 2);
		a[N++] = item;
	}
	
	private void resize(int M) {
		Item[] copy = (Item[]) new Object[M];
		for (int i = 0; i < N; i++) {
			copy[i] = a[i];
		}
		a = copy;
	}
	
}
